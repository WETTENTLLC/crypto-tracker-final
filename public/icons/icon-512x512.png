<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="cryptoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1d4ed8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fbbf24;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f59e0b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="256" cy="256" r="240" fill="url(#cryptoGradient)" stroke="#1e40af" stroke-width="8"/>
  
  <!-- CryptoTracker symbol -->
  <g transform="translate(256,256)">
    <!-- Chart icon representing tracking -->
    <path d="M -80,-60 L -40,-20 L 0,-40 L 40,0 L 80,-20" 
          fill="none" stroke="#ffffff" stroke-width="8" stroke-linecap="round" stroke-linejoin="round"/>
    
    <!-- Circle markers for data points -->
    <circle cx="-80" cy="-60" r="8" fill="#fbbf24"/>
    <circle cx="-40" cy="-20" r="8" fill="#fbbf24"/>
    <circle cx="0" cy="-40" r="8" fill="#fbbf24"/>
    <circle cx="40" cy="0" r="8" fill="#fbbf24"/>
    <circle cx="80" cy="-20" r="8" fill="#fbbf24"/>
    
    <!-- Alert bell symbol -->
    <path d="M -15,40 Q -15,25 0,25 Q 15,25 15,40 L 15,55 Q 15,65 5,65 L -5,65 Q -15,65 -15,55 Z M -20,65 L 20,65" 
          fill="#ffffff" stroke="#f59e0b" stroke-width="2"/>
    <circle cx="0" cy="75" r="4" fill="#f59e0b"/>
  </g>
  
  <!-- Decorative crypto symbols -->
  <text x="100" y="100" font-family="Arial, sans-serif" font-size="24" fill="#fbbf24" opacity="0.6">₿</text>
  <text x="380" y="120" font-family="Arial, sans-serif" font-size="20" fill="#fbbf24" opacity="0.5">Ξ</text>
  <text x="120" y="420" font-family="Arial, sans-serif" font-size="18" fill="#fbbf24" opacity="0.4">₳</text>
</svg>
